{
  "extends": "standard",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "modules": true
    }
  },
  "plugins": [
    "jest"
  ],
  "env": {
    "jest": true
  },
  "rules": {
    "array-bracket-spacing": [ "error", "always" ],
    "object-curly-spacing": [ "error", "always" ]
  }
}
